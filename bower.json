{
  "name": "hoopsworthy",
  "version": "0.1.0",
  "dependencies": {
    "wp-normalize.scss": "~1.0.2",
    "bourbon": "~4.2.4",
    "neat": "~1.7.2",
    "modernizr": "~2.8.3",
    "fitvids": "~1.1.0",
    "bower-webfontloader": "~1.5.16"
  },
  "ignore": [
    "node_modules",
    "components",
    "lib"
  ],
  "exportsOverride": {
    "wp-normalize.scss": {
      "scss": "_wp-normalize.scss"
    },
    "bourbon": {
      "scss": "app/assets/stylesheets"
    },
    "neat": {
      "scss": "app/assets/stylesheets"
    },
    "modernizr": {
      "modernizr": "modernizr.js"
    },
    "responsive-nav": {
      "scss": "*.css",
      "js": "*.js"
    },
    "fitvids": {
      "js": "jquery.fitvids.js"
    },
    "bower-webfontloader": {
      "js": "webfont.js"
    }
  }
}
